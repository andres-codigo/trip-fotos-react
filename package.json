{
  "name": "react-starter-project",
  "private": true,
  "version": "0.12.3",
  "description": "Conversion to React and extension of Udemy's Vue - The Complete Guide (incl. Router & Composition API) 'Find a coach' project.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0 && prettier --write '**/*.{js,jsx,ts,tsx,config.js,config.mjs,css,scss,json,md}'",
	"format": "prettier --write '**/*.{js,jsx,ts,tsx,config.js,config.mjs,css,scss,json,md}'",
    "check:dependencies": "mkdir -p scripts/dependency-check && depcheck --txt > scripts/dependency-check/depcheck-output.txt",
    "cy:open": "cypress open",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
    "@vercel/analytics": "^1.5.0",
    "@vitest/coverage-v8": "^3.1.2",
    "classnames": "^2.5.1",
    "dotenv": "^16.5.0",
    "firebase": "^11.3.1",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.2",
    "react-transition-group": "^4.4.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/eslint": "^9.6.1",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.4.0",
    "chokidar-cli": "^3.0.0",
    "cypress": "^14.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "react-icons": "^5.5.0",
    "sass-embedded": "^1.85.0",
    "vite": "^6.3.4",
    "vite-plugin-eslint2": "^5.0.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": "20.x"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
